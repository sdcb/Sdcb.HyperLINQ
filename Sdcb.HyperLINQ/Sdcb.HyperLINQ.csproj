<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ImplicitUsings>false</ImplicitUsings>
    <Nullable>disable</Nullable>
    <LangVersion>10.0</LangVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/sdcb/Sdcb.HyperLINQ</PackageProjectUrl>
    <RepositoryUrl>https://github.com/sdcb/Sdcb.HyperLINQ</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>html;template-engine;</PackageTags>
    <VersionPrefix>1.0.01</VersionPrefix>
    <Description>Hyperlinq is a C# library for tersely constructing the elements and attributes that comprise an HTML DOM. </Description>
    <Authors>sdcb;</Authors>
  </PropertyGroup>

</Project>
